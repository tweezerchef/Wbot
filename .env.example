# ============================================================================
# TBot Environment Variables
# ============================================================================
# Copy this file to .env and fill in your values
# NEVER commit .env to version control
# ============================================================================

# ----------------------------------------------------------------------------
# Supabase Configuration
# ----------------------------------------------------------------------------
# Get these from your Supabase project dashboard:
# https://app.supabase.com/project/_/settings/api

# Project URL (e.g., https://xyz.supabase.co)
SUPABASE_URL=

# Anonymous/public key - safe to expose in browser
SUPABASE_ANON_KEY=

# Service role key - NEVER expose to client, server-side only
SUPABASE_SERVICE_KEY=

# ----------------------------------------------------------------------------
# Vite Client-Side Variables
# ----------------------------------------------------------------------------
# Variables prefixed with VITE_ are exposed to the browser
# These should match the Supabase values above

VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# ----------------------------------------------------------------------------
# LLM API Keys
# ----------------------------------------------------------------------------
# At least one is required for the AI backend

# Anthropic Claude API key
# Get from: https://console.anthropic.com/
ANTHROPIC_API_KEY=

# Google AI API key (optional, for Gemini)
# Get from: https://makersuite.google.com/app/apikey
GOOGLE_API_KEY=

# ----------------------------------------------------------------------------
# LangGraph Configuration
# ----------------------------------------------------------------------------
# URL of the LangGraph API server

# Local development
LANGGRAPH_API_URL=http://localhost:8000
VITE_LANGGRAPH_API_URL=http://localhost:8000

# Production (update after deploying AI backend)
# LANGGRAPH_API_URL=https://your-deployment.langchain.app
# VITE_LANGGRAPH_API_URL=https://your-deployment.langchain.app
