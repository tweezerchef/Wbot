# ============================================================================
# Wbot Environment Variables
# ============================================================================
# Copy this file to .env and fill in your values
# NEVER commit .env to version control
# ============================================================================

# ----------------------------------------------------------------------------
# Supabase Configuration
# ----------------------------------------------------------------------------
# Get these from your Supabase project dashboard:
# https://app.supabase.com/project/_/settings/api

# Project URL (e.g., https://xyz.supabase.co)
SUPABASE_URL=

# Anonymous/public key - safe to expose in browser
SUPABASE_ANON_KEY=

# Service role key - NEVER expose to client, server-side only
SUPABASE_SERVICE_KEY=

# Database password for checkpointing
# Get from: Supabase Dashboard > Settings > Database
SUPABASE_DB_PASSWORD=

# PostgreSQL connection string for LangGraph checkpointing
# Use Supavisor Session Mode pooler (IPv4 compatible - works in Docker)
# Format: postgresql://postgres.[ref]:[password]@aws-0-[region].pooler.supabase.com:5432/postgres
# Get from: Supabase Dashboard > Settings > Database > Connection string (Session mode)
DATABASE_URI=

# ----------------------------------------------------------------------------
# Redis Configuration
# ----------------------------------------------------------------------------
# Used for embedding cache and server-side caching
# Use Upstash (https://upstash.com) for remote Redis or run locally

# Primary Redis URL (used by AI backend for embedding cache)
REDIS_URL=

# Shared Redis URL (used by both web and AI backend for shared caching)
# Typically the same as REDIS_URL when using a remote Redis like Upstash
REDIS_SHARED_URL=

# ----------------------------------------------------------------------------
# Vite Client-Side Variables
# ----------------------------------------------------------------------------
# Variables prefixed with VITE_ are exposed to the browser
# These should match the Supabase values above

VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# ----------------------------------------------------------------------------
# LLM API Keys
# ----------------------------------------------------------------------------
# At least ANTHROPIC_API_KEY is required for the AI backend

# Anthropic Claude API key (required)
# Get from: https://console.anthropic.com/
ANTHROPIC_API_KEY=

# Google AI API key (optional, for Gemini)
# Get from: https://makersuite.google.com/app/apikey
GOOGLE_API_KEY=

# OpenAI API key (optional, for meditation TTS)
# Get from: https://platform.openai.com/api-keys
OPENAI_API_KEY=

# ZhipuAI API key (optional, for GLM models)
# Get from: https://z.ai
ZAI_API_KEY=

# ----------------------------------------------------------------------------
# LangGraph / LangSmith Configuration
# ----------------------------------------------------------------------------
# Local development: http://localhost:2024 (both uvicorn and Docker use this port)
# Production: Update to your deployed URL
VITE_LANGGRAPH_API_URL=http://localhost:2024

# LangSmith tracing (optional - sends traces to https://smith.langchain.com)
# Get API key from: https://smith.langchain.com/settings
LANGSMITH_TRACING=false
LANGSMITH_PROJECT=wbot-wellness
LANGSMITH_API_KEY=

# ----------------------------------------------------------------------------
# Storybook Test User (Development Only)
# ----------------------------------------------------------------------------
# Credentials for the test user used in Storybook stories.
# Create a user in Supabase with these credentials and seed some test data.
VITE_STORYBOOK_TEST_EMAIL=
VITE_STORYBOOK_TEST_PASSWORD=
