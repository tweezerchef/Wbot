{
  "name": "wbot",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "turbo dev",
    "dev:all": "concurrently -n web,ai -c blue,green \"pnpm dev:web\" \"pnpm dev:ai\"",
    "dev:web": "turbo dev --filter=@wbot/web",
    "dev:ai": "cd apps/ai && uv run langgraph dev",
    "storybook": "pnpm --filter @wbot/storybook dev",
    "storybook:build": "pnpm --filter @wbot/storybook build",
    "docs": "pnpm --filter @wbot/docs dev",
    "docs:build": "pnpm --filter @wbot/docs build",
    "docs:generate": "cd apps/ai && uv run python ../../scripts/ai_docs/generator.py",
    "docs:generate:full": "cd apps/ai && uv run python ../../scripts/ai_docs/generator.py --full",
    "docs:check-stale": "cd apps/ai && uv run python ../../scripts/ai_docs/staleness.py",
    "build": "turbo build",
    "lint": "turbo lint",
    "lint:fix": "turbo lint -- --fix",
    "test": "turbo test",
    "test:web": "pnpm --filter @wbot/web test",
    "test:ai": "cd apps/ai && uv run pytest",
    "test:coverage": "turbo test:coverage",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "deps:install": "./scripts/install-deps.sh",
    "clean": "turbo clean",
    "db:push": "pnpm exec supabase db push --linked",
    "db:status": "pnpm exec supabase migration list --linked",
    "db:generate-types": "pnpm exec supabase gen types typescript --linked > packages/shared/src/types/database.ts",
    "db:diff": "pnpm exec supabase db diff --use-migra --linked",
    "db:new": "pnpm exec supabase migration new",
    "db:link": "pnpm exec supabase link",
    "db:local:start": "pnpm exec supabase start",
    "db:local:stop": "pnpm exec supabase stop",
    "db:local:reset": "pnpm exec supabase db reset",
    "redis:local:start": "docker run -d --name wbot-redis -p 6379:6379 redis:alpine && echo 'Redis started on localhost:6379'",
    "redis:local:stop": "docker stop wbot-redis && docker rm wbot-redis && echo 'Redis stopped'",
    "redis:local:status": "docker ps --filter name=wbot-redis --format '{{.Status}}' || echo 'Redis not running'",
    "prepare": "husky"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "rollup-plugin-visualizer": "^6.0.5",
    "supabase": "^2.70.5",
    "tsx": "^4.21.0",
    "turbo": "^2.7.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ],
    "apps/ai/**/*.py": [
      "bash -c 'cd apps/ai && uv run ruff check --fix'",
      "bash -c 'cd apps/ai && uv run ruff format'"
    ]
  },
  "packageManager": "pnpm@9.15.1",
  "engines": {
    "node": ">=20"
  }
}
