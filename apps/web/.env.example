# ============================================================================
# Web App Environment Variables
# ============================================================================
# Copy this file to .env and fill in your values
# IMPORTANT: VITE_ prefix is required for variables exposed to the client
# ============================================================================

# Supabase Configuration
# Get from: https://app.supabase.com/project/_/settings/api
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# LangGraph API (AI Backend)
# Local development: http://localhost:2024 (LangGraph CLI default port)
# Production: Your LangGraph Cloud deployment URL
VITE_LANGGRAPH_API_URL=http://localhost:2024
